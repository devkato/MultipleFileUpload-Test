<h1>Listing upload_files</h1>

<table>
  <tr>
    <th>File file name</th>
    <th>File content type</th>
    <th>Content file size</th>
    <th>Content updated at</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @upload_files.each do |upload_file| %>
  <tr>
    <td><%= upload_file.file_file_name %></td>
    <td><%= upload_file.file_content_type %></td>
    <td><%= upload_file.content_file_size %></td>
    <td><%= upload_file.content_updated_at %></td>
    <td><%= link_to 'Show', upload_file %></td>
    <td><%= link_to 'Edit', edit_upload_file_path(upload_file) %></td>
    <td><%= link_to 'Destroy', upload_file, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Upload file', new_upload_file_path %>
